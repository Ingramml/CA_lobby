{
  "version": 2,
  "name": "ca-lobby-demo",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "functions": {
    "backend/app.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "./backend"
  },
  "github": {
    "enabled": true
  }
}
