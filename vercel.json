{
  "buildCommand": "cd webapp/frontend && npm run build",
  "outputDirectory": "webapp/frontend/build",
  "functions": {
    "webapp/backend/app.py": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/webapp/backend/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./webapp/backend"
  }
}