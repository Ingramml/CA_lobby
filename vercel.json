{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "webapp/frontend/build"
      }
    },
    {
      "src": "webapp/backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/webapp/backend/app.py"
    },
    {
      "src": "/static/js/(.*)",
      "dest": "/webapp/frontend/build/static/js/$1"
    },
    {
      "src": "/static/css/(.*)",
      "dest": "/webapp/frontend/build/static/css/$1"
    },
    {
      "src": "/static/media/(.*)",
      "dest": "/webapp/frontend/build/static/media/$1"
    },
    {
      "src": "/(favicon\\.ico|logo192\\.png|logo512\\.png|manifest\\.json|robots\\.txt)",
      "dest": "/webapp/frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/webapp/frontend/build/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "./webapp/backend"
  }
}